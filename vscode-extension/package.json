{
  "name": "aicache-vscode",
  "displayName": "aicache",
  "description": "AI CLI Session Caching for VS Code",
  "version": "0.1.0",
  "publisher": "aicache",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "aicache.queryCache",
        "title": "Query aicache",
        "category": "aicache"
      },
      {
        "command": "aicache.showCachePanel",
        "title": "Show Cache Panel",
        "category": "aicache"
      },
      {
        "command": "aicache.refreshCache",
        "title": "Refresh Cache",
        "category": "aicache"
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "aicache",
          "title": "aicache",
          "icon": "resources/aicache-icon.svg"
        }
      ]
    },
    "views": {
      "aicache": [
        {
          "id": "aicache.cachePanel",
          "name": "Cache Browser",
          "when": "aicache:enabled"
        },
        {
          "id": "aicache.teamPanel",
          "name": "Team Presence",
          "when": "aicache:collaborationEnabled"
        }
      ]
    },
    "menus": {
      "editor/context": [
        {
          "command": "aicache.queryCache",
          "when": "editorTextFocus",
          "group": "navigation"
        }
      ]
    },
    "keybindings": [
      {
        "command": "aicache.queryCache",
        "key": "ctrl+shift+a",
        "mac": "cmd+shift+a"
      }
    ],
    "configuration": {
      "title": "aicache",
      "properties": {
        "aicache.enabled": {
          "type": "boolean",
          "default": true,
          "description": "Enable aicache extension"
        },
        "aicache.serviceUrl": {
          "type": "string",
          "default": "http://localhost:8080",
          "description": "URL of the aicache service"
        },
        "aicache.enableCollaboration": {
          "type": "boolean",
          "default": false,
          "description": "Enable team collaboration features"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.74.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.45.0",
    "eslint": "^8.28.0",
    "typescript": "^4.9.3",
    "mocha": "^10.1.0"
  },
  "dependencies": {
    "axios": "^1.3.4"
  }
}